}

.slide-image{
  width: 750rpx;
  height: 340rpx;
}

.nav{
  width:100%;
  padding:10rpx 0;
  display: flex;
  justify-content:space-around;
}
.nav image{
  width: 128rpx;
  height: 140rpx;
}
.floors{
