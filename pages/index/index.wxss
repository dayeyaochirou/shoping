  height:386rpx;
}
.floors-right{
  margin-left: 25rpx;
  flex: 1;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
} 
.floors-right image{
  width:233rpx;
  height:188rpx;
}
